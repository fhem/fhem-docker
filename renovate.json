{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "docker:pinDigests"
  ],
  "packageRules": [
    {
      "fileMatch": [
        "(^|\/)Dockerfile.*"
      ],
      "matchDatasources": ["docker"],
      "allowedVersions": "^(5\\.)([1-9][02468])(\\.\\d)?(.*)$"
    }
  ]
}
